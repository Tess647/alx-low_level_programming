#include <stdio.h>

/**
 * main - Entry point of the program
 *
 * This program displays the lower and uppercase letters
 * Return : always 0 (Success)
 */
int main(void)
{
	char ch = 'a';
	while (ch <= 'z')
	{
		putchar(ch);
	}
	char ch = 'A';
	while (ch <= 'Z')
	{
		putchar(ch);
	}
	putchar('/n');
	return (0);
}
